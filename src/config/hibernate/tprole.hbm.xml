<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kfj.entity.TpRole" table="tprole">
        <id name="id" column="id" type="java.lang.Integer"></id>
        <property name="openId" type="java.lang.String" column="openId"/>       
        <property name="babyId" type="java.lang.Integer" column="babyId"/>     
    </class>

</hibernate-mapping>