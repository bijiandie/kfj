<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kfj.entity.WxUser" table="WxUser">
        <id name="id" column="id" type="java.lang.Integer"></id>
        <property name="OPENID" type="java.lang.String" column="OPENID"/>       
        <property name="gzState" type="java.lang.String" column="gzState"/> 
        <property name="tpstate" type="java.lang.String" column="tpstate"/>  
        <property name="tptime" type="java.lang.String" column="tptime"/>           
    </class>
</hibernate-mapping>